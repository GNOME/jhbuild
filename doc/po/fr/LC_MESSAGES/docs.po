#, no-wrap
msgid ""
"\n"
"&lt;repository type=\"bzr\" name=\"launchpad.net\"\n"
"      href=\"lp:\"\n"
"      trunk-template=\"~bzr-pqm/%(module)s/bzr.dev\"\n"
"      branches-template=\"~bzr-pqm/%(module)s/%(branch)s\"/&gt;\n"
"        "
msgstr ""
"\n"
"&lt;repository type=\"bzr\" name=\"launchpad.net\"\n"
"      href=\"lp:\"\n"
"      trunk-template=\"~bzr-pqm/%(module)s/bzr.dev\"\n"
"      branches-template=\"~bzr-pqm/%(module)s/%(branch)s\"/&gt;\n"
"        "

