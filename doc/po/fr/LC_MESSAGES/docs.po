#, no-wrap
msgid ""
"\n"
"&lt;branch repo=\"launchpad.net\"\n"
"      module=\"bzr\"\n"
"      checkoutdir=\"bzr-next\"/&gt;\n"
"        "
msgstr ""
"\n"
"&lt;branch repo=\"launchpad.net\"\n"
"      module=\"bzr\"\n"
"      checkoutdir=\"bzr-next\"/&gt;\n"
"        "

#: C/jhbuild.xml:2472(programlisting)
#, no-wrap
msgid ""
"\n"
"&lt;branch repo=\"launchpad.net\"\n"
