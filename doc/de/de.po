"&lt;waf id=\"<replaceable>modulename</replaceable>\"&gt;\n"
"\t [ waf-command=\"<replaceable>waf-command</replaceable>\" ]&gt;\n"
"  &lt;branch [ ... ] &gt;\n"
"    [...]\n"
"  &lt;/branch&gt;\n"
