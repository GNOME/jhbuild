      <dep package="gtk+-3"/>
      <dep package="gtk-doc"/>
      <dep package="m4-common"/>
      <dep package="WebKit"/>
    </dependencies>
  </autotools>

  <cmake id="evolution"
         cmakeargs="-DENABLE_LIBCRYPTUI=OFF -DENABLE_PST_IMPORT=OFF -DENABLE_YTNEF=OFF">
    <branch/>
    <dependencies>
      <dep package="atk"/>
      <dep package="bogofilter"/>
      <dep package="cairo"/>
      <dep package="enchant"/>
      <dep package="evolution-data-server"/>
