    </dependencies>
  </autotools>

  <autotools id="gnome-builder">
    <branch/>
    <dependencies>
      <dep package="clang"/>
      <dep package="devhelp"/>
      <dep package="gjs"/>
      <dep package="glib"/>
      <dep package="glib-networking"/>
      <dep package="gnome-code-assistance"/>
      <dep package="gspell"/>
      <dep package="gtk+-3"/>
      <dep package="gtksourceview-3"/>
      <dep package="libgit2-glib"/>
      <dep package="libpeas"/>
      <dep package="librsvg"/>
      <dep package="llvm"/>
      <dep package="pygobject"/>
      <dep package="vala"/>
