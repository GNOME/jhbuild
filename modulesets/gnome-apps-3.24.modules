      <dep package="hitori"/>
      <dep package="iagno"/>
      <dep package="lightsoff"/>
      <dep package="quadrapassel"/>
      <dep package="swell-foop"/>
      <dep package="tali"/>
    </dependencies>
  </metamodule>

