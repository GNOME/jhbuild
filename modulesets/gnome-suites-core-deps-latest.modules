      <dep package="libarchive"/>
      <dep package="libgusb"/>
      <dep package="libsoup"/>
      <dep package="polkit-gobject"/>
      <dep package="sqlite3"/>
      <dep package="gpgme"/>
      <dep package="docbook-utils"/>
      <dep package="libgusb"/>
      <dep package="libelf"/>
      <dep package="fwupdate"/>
    </dependencies>
  </meson>

  <meson id="gcab">
    <branch/>
    <dependencies>
      <dep package="glib"/>
      <dep package="gobject-introspection"/>
      <dep package="vala"/>
      <dep package="zlib"/>
    </dependencies>
  </meson>

  <meson id="gexiv2"
         mesonargs="-Dgtk_doc=false -Dpython2_girdir=no">
    <branch/>
    <dependencies>
      <dep package="exiv2"/>
      <dep package="glib"/>
      <dep package="gobject-introspection"/>
      <dep package="vala"/>
      <dep package="pygobject"/>
    </dependencies>
  </meson>

  <meson id="glade">
    <if condition-set="macos">
      <mesonargs value="-Dman=false"/>
    </if>
    <branch/>
    <dependencies>
      <dep package="gtk+-3"/>
      <dep package="libxml2"/>
      <dep package="yelp-tools"/>
      <if condition-set="macos">
        <dep package="gtk-mac-integration"/>
      </if>
    </dependencies>
  </meson>

 <autotools id="gnome-autoar">
   <branch/>
