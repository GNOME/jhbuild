      <dep package="freetype2"/>
      <dep package="gcab"/>
      <dep package="gdk-pixbuf"/>
      <dep package="glib"/>
      <dep package="gobject-introspection"/>
      <dep package="gperf"/>
      <dep package="gtk+-3"/>
      <dep package="json-glib"/>
      <dep package="libarchive"/>
      <dep package="libsoup"/>
      <dep package="libuuid"/>
      <dep package="libyaml"/>
      <dep package="sqlite3"/>
    </dependencies>
  </meson>

  <meson id="atk">
    <branch/>
    <dependencies>
      <dep package="glib"/>
      <dep package="gobject-introspection"/>
      <dep package="gtk-doc"/>
    </dependencies>
  </meson>

  <autotools id="atkmm-1.6">
