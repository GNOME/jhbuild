      <dep package="yelp-tools"/>
      <dep package="libmanette"/>
    </dependencies>
  </autotools>

  <autotools id="swell-foop">
    <branch/>
    <dependencies>
      <dep package="appstream-glib"/>
      <dep package="intltool"/>
      <dep package="gtk+-3"/>
      <dep package="clutter"/>
      <dep package="clutter-gtk"/>
      <dep package="vala"/>
      <dep package="yelp-tools"/>
