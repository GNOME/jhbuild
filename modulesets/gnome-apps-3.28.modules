      <dep package="librsvg"/>
      <dep package="gtk+-3"/>
      <dep package="yelp-tools"/>
    </dependencies>
  </autotools>

  <autotools id="gnome-mahjongg">
    <branch/>
    <dependencies>
      <dep package="appstream-glib"/>
