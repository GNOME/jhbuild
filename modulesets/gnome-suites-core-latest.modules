        <dep package="udisks"/>
      </if>
      <if condition-set="systemd">
        <dep package="libsystemd"/>
      </if>
      <if condition-set="udev">
        <dep package="libcdio-paranoia"/>
        <dep package="libgphoto2"/>
        <dep package="libgudev"/>
        <dep package="libmtp"/>
      </if>
      <if condition-unset="freebsd">
        <dep package="libusb1"/>
      </if>
    </dependencies>
  </meson>

