      <if condition-set="linux">
        <dep package="libgusb"/>
        <dep package="libusb1"/>
        <dep package="gudev"/>
      </if>
      <if condition-set="systemd">
        <dep package="libsystemd"/>
      </if>
    </dependencies>
    <suggests>
      <dep package="polkit-gobject"/>
    </suggests>
  </autotools>

  <autotools id="colord-gtk">
